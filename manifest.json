{
   "manifest_version": 2,
   "name": "synaesthesia",
   "description": "A chrome extension that changes the default colours of the GAE admin console based on app ID.",
   "version": "0.1",
   "options_page": "./html/options.html",
   "background" : {
      "scripts": ["./js/vendor/jquery-1.10.2.min.js",
                  "./js/synaesthete.js"]
  },
   "page_action": {
      "default_icon": {
         "19": "./images/page_icon.png"
      }
   },
   "permissions": [
      "tabs", "storage", "https://appengine.google.com/*"
   ]
}
